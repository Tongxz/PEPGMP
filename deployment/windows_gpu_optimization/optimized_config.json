{
  "yolo_optimization": {
    "model_format": "pytorch",
    "precision": "fp16",
    "batch_size": 4,
    "imgsz": 640,
    "device": "cuda:0",
    "compile": true,
    "half": true,
    "dnn": true,
    "augment": false,
    "agnostic_nms": false,
    "retina_masks": true
  },
  "mediapipe_optimization": {
    "gpu_acceleration": true,
    "model_complexity": 1,
    "min_detection_confidence": 0.5,
    "min_tracking_confidence": 0.5,
    "max_num_hands": 2,
    "static_image_mode": false
  },
  "parallel_processing": {
    "enable_threading": true,
    "max_workers": 2,
    "queue_size": 32,
    "prefetch_factor": 2,
    "frame_skip_strategy": "adaptive",
    "roi_tracking": true
  },
  "memory_optimization": {
    "frame_buffer_size": 10,
    "result_cache_size": 50,
    "similarity_threshold": 0.95,
    "garbage_collection_interval": 100,
    "preallocate_memory": true
  }
}
